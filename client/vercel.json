{
  "version": 2,
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/ftp-manager.html",
      "destination": "/ftp-manager.html"
    },
    {
      "source": "/ftp-manager.css",
      "destination": "/ftp-manager.css"
    },
    {
      "source": "/ftp-manager.js",
      "destination": "/ftp-manager.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}



